
year <- sub("-.*", "", meta$Date)note <- sprintf("R package version %s", meta$Version)
bibentry(bibtype = "Manual",title = "{astrolabe}: Astronomy Users Library for R",author = c(person("Arnab", "Chakraborty"),person("Eric D.", "Feigelson"),person("G. Jogesh", "Babu")),year = year,note = note,url = "http://CRAN.R-project.org/package=astrolibR")